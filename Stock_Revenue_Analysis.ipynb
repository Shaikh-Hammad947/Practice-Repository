    "# Stock and Revenue Data Analysis for Tesla and GameStop"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import yfinance as yf\n",
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Step 1: Extract Tesla Stock Data\n",
    "tesla_stock = yf.download(\"TSLA\", start=\"2015-01-01\", end=\"2023-12-31\")\n",
    "tesla_stock.reset_index(inplace=True)\n",
    "\n",
    "# Save Tesla Stock Data\n",
    "tesla_stock.to_csv("Tesla_Stock.csv", index=False)\n",
    "\n",
    "# Step 2: Extract Tesla Revenue Data via Web Scraping\n",
    "tesla_revenue_url = \"https://www.macrotrends.net/stocks/charts/TSLA/tesla/revenue\"\n",
    "tesla_response = requests.get(tesla_revenue_url)\n",
    "tesla_soup = BeautifulSoup(tesla_response.content, \"html.parser\")\n",
    "tesla_table = tesla_soup.find(\"table\", {\"class\": \"historical_data_table table\"})\n",
    "\n",
    "# Parse Tesla Revenue Data\n",
    "tesla_revenue_data = []\n",
    "for row in tesla_table.find_all(\"tr\"):\n",
    "    columns = row.find_all(\"td\")\n",
    "    if len(columns) == 2:\n",
    "        date = columns[0].text.strip()\n",
    "        revenue = columns[1].text.strip().replace(\"$\", \"\").replace(\",\", \"\")\n",
    "        tesla_revenue_data.append([date, revenue])\n",
    "\n",
    "tesla_revenue_df = pd.DataFrame(tesla_revenue_data, columns=[\"Date\", \"Revenue\"])\n",
    "tesla_revenue_df[\"Date\"] = pd.to_datetime(tesla_revenue_df[\"Date\"])\n",
    "tesla_revenue_df[\"Revenue\"] = pd.to_numeric(tesla_revenue_df[\"Revenue\"], errors=\"coerce\")\n",
    "\n",
    "# Save Tesla Revenue Data\n",
    "tesla_revenue_df.to_csv("Tesla_Revenue.csv", index=False)\n",
    "\n",
    "# Step 3: Extract GameStop Stock Data\n",
    "gamestop_stock = yf.download(\"GME\", start=\"2015-01-01\", end=\"2023-12-31\")\n",
    "gamestop_stock.reset_index(inplace=True)\n",
    "\n",
    "# Save GameStop Stock Data\n",
    "gamestop_stock.to_csv("GameStop_Stock.csv", index=False)\n",
    "\n",
    "# Step 4: Extract GameStop Revenue Data via Web Scraping\n",
    "gamestop_revenue_url = \"https://www.macrotrends.net/stocks/charts/GME/gamestop/revenue\"\n",
    "gamestop_response = requests.get(gamestop_revenue_url)\n",
    "gamestop_soup = BeautifulSoup(gamestop_response.content, \"html.parser\")\n",
    "gamestop_table = gamestop_soup.find(\"table\", {\"class\": \"historical_data_table table\"})\n",
    "\n",
    "# Parse GameStop Revenue Data\n",
    "gamestop_revenue_data = []\n",
    "for row in gamestop_table.find_all(\"tr\"):\n",
    "    columns = row.find_all(\"td\")\n",
    "    if len(columns) == 2:\n",
    "        date = columns[0].text.strip()\n",
    "        revenue = columns[1].text.strip().replace(\"$\", \"\").replace(\",\", \"\")\n",
    "        gamestop_revenue_data.append([date, revenue])\n",
    "\n",
    "gamestop_revenue_df = pd.DataFrame(gamestop_revenue_data, columns=[\"Date\", \"Revenue\"])\n",
    "gamestop_revenue_df[\"Date\"] = pd.to_datetime(gamestop_revenue_df[\"Date\"])\n",
    "gamestop_revenue_df[\"Revenue\"] = pd.to_numeric(gamestop_revenue_df[\"Revenue\"], errors=\"coerce\")\n",
    "\n",
    "# Save GameStop Revenue Data\n",
    "gamestop_revenue_df.to_csv("GameStop_Revenue.csv", index=False)\n",
    "\n",
    "# Create Tesla Dashboard\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(tesla_stock[\"Date\"], tesla_stock[\"Close\"], label=\"Tesla Stock Price\", color=\"blue\")\n",
    "plt.bar(tesla_revenue_df[\"Date\"], tesla_revenue_df[\"Revenue\"], label=\"Tesla Revenue\", color=\"orange\", alpha=0.5)\n",
    "plt.title(\"Tesla Stock Price vs Revenue\")\n",
    "plt.xlabel(\"Date\")\n",
    "plt.ylabel(\"Values\")\n",
    "plt.legend()\n",
    "plt.grid()\n",
    "plt.savefig("Tesla_Dashboard.png")\n",
    "\n",
    "# Create GameStop Dashboard\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.plot(gamestop_stock[\"Date\"], gamestop_stock[\"Close\"], label=\"GameStop Stock Price\", color=\"green\")\n",
    "plt.bar(gamestop_revenue_df[\"Date\"], gamestop_revenue_df[\"Revenue\"], label=\"GameStop Revenue\", color=\"red\", alpha=0.5)\n",
    "plt.title(\"GameStop Stock Price vs Revenue\")\n",
    "plt.xlabel(\"Date\")\n",
    "plt.ylabel(\"Values\")\n",
    "plt.legend()\n",
    "plt.grid()\n",
    "plt.savefig("GameStop_Dashboard.png")"
   ]
  }
